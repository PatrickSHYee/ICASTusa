            <a href="@Url.Action("Case_Studies", "Success_Stories")">SUCCESS STORIES CASE STUDIES</a> |
            <a href="#">SUCCESS STORIES PAST PROJECTS</a> |
            <a href="@Url.Action("Testimonials", "Success_Stories")">SUCCESS STORIES TESTIMONIALS</a> |
            <a href="@Url.Action("The_ICAST_Board", "About")">THE ICAST BOARD</a> |
            <a href="@Url.Action("The_ICAST_Team", "About")">THE ICAST TEAM</a> |
            <a href="@Url.Action("Training_and_Resources", "Programs")">TRAINING AND RESOURCES</a> |
            <a href="@Url.Action("Who_We_Are", "About")">WHO WE ARE</a> |
            <a href="#">LOGIN</a>
            <p>&copy; @DateTime.Now.Year - ICAST All Rights Reserved <a href="">Site Map</a></p>
        </footer>
    </div>
    <div class="row bottomNav">
        <div class="navbar navbar-inverse navbar-fixed-bottom">
            <div class="container">
                <div class="col-md-4 text-left">
                    <ul class="nav navbar-nav">
                        <li><a href="#"><span class="glyphicon glyphicon-comment"></span> View Blog</a></li>
                        <li><a href="#"><span class="glyphicon glyphicon-calendar"></span> Events</a></li>
                    </ul>
                </div>
                <div class="col-md-4"></div>
                <div class="col-md-4 text-right">
                    <ul class="nav navbar-nav">
                        <li><a href="https://plus.google.com/110678628120966349089/about?hl=en"><span class="fa fa-google-plus-square fa-2x"></span></a></li>
                        <li><a href="https://www.facebook.com/icastusa"><span class="fa fa-facebook-square fa-2x"></span></a></li>
                        <li><a href="https://twitter.com/ICASTusa"><span class="fa fa-twitter-square fa-2x"></span></a></li>
                        <li><a href="https://www.linkedin.com/company/icast?trk=top_nav_home"><span class="fa fa-linkedin-square fa-2x"></span></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/site")
